<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.onlineordering"
    android:installLocation="preferExternal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FruitsVegetables" android:label="Fruits and beverages"> </activity>
        <activity android:name=".Beverages" android:label="Beverages" >

        </activity>
        <activity android:name=".listitems" android:label="List Items"></activity>
        <activity android:name=".BrandedFoods" android:label="Branded Foods"></activity>
        <activity android:name=".Groceries" android:label="Groceries"></activity>
        <activity android:name=".HouseHold" android:label="House Hold"></activity>

    </application>

</manifest>